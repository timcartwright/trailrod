<% if @admin %>
  $('#registration-form').replaceWith('<%= j render('event_registrations/form') %>');
  $('select').material_select();
  $('#modal1').closeModal();
<% else %>
  $('<%= "#new-profile-form-#{@event.id}" %>').replaceWith("<%= j render 'event_registrations/payment' %>");
<% end %>

$(".preloader").hide();